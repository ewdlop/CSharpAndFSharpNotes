SELECT RX_PATIENT_DMREFID as pat_id,RX_PACKAGE_DMREFID as code_id,PRODUCT_PROPRIETARY_NAME AS DESCRIPTION,RX_FILL_DATE AS SVC_DATE,INDEX_DATE,
